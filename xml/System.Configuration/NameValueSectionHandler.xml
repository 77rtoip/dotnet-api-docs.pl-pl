      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Configuration.NameValueSectionHandler">
          <source>Provides name/value-pair configuration information from a configuration section.</source>
          <target state="translated">Zawiera informacje o konfiguracji para nazwa wartość z sekcji konfiguracji.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Configuration.NameValueSectionHandler">
          <source>This class implements the <ph id="ph1">`IConfigSectionHandler`</ph> interface.</source>
          <target state="translated">Ta klasa implementuje <ph id="ph1">`IConfigSectionHandler`</ph> interfejsu.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Configuration.NameValueSectionHandler">
          <source>The <ph id="ph1">&lt;xref:System.Configuration.NameValueSectionHandler&gt;</ph> defines rules for manipulating XML configuration sections.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Configuration.NameValueSectionHandler&gt;</ph> Definiuje reguły manipulowanie XML sekcji konfiguracyjnych.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Configuration.NameValueSectionHandler.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Configuration.NameValueSectionHandler" /&gt;</ph> class.</source>
          <target state="translated">Inicjuje nowe wystąpienie klasy <ph id="ph1">&lt;see cref="T:System.Configuration.NameValueSectionHandler" /&gt;</ph> klasy.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Configuration.NameValueSectionHandler.Create(System.Object,System.Object,System.Xml.XmlNode)">
          <source>Parent object.</source>
          <target state="translated">Obiekt nadrzędny.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Configuration.NameValueSectionHandler.Create(System.Object,System.Object,System.Xml.XmlNode)">
          <source>Configuration context object.</source>
          <target state="translated">Obiekt kontekstu konfiguracji.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Configuration.NameValueSectionHandler.Create(System.Object,System.Object,System.Xml.XmlNode)">
          <source>Section XML node.</source>
          <target state="translated">Węzeł XML sekcji.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Configuration.NameValueSectionHandler.Create(System.Object,System.Object,System.Xml.XmlNode)">
          <source>Creates a new configuration handler and adds it to the section-handler collection based on the specified parameters.</source>
          <target state="translated">Tworzy nowy program obsługi konfiguracji i dodaje go do kolekcji modułu obsługi sekcji, na podstawie określonych parametrów.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Configuration.NameValueSectionHandler.Create(System.Object,System.Object,System.Xml.XmlNode)">
          <source>A configuration object.</source>
          <target state="translated">Obiekt konfiguracji.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Configuration.NameValueSectionHandler.Create(System.Object,System.Object,System.Xml.XmlNode)">
          <source>The <ph id="ph1">`parent`</ph> object parameter acts as the configuration settings in the corresponding parent configuration section.</source>
          <target state="translated"><ph id="ph1">`parent`</ph> Parametr obiektu zachowuje się jak ustawienia konfiguracji w odpowiedniej sekcji konfiguracji nadrzędnej.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Configuration.NameValueSectionHandler.Create(System.Object,System.Object,System.Xml.XmlNode)">
          <source>The <ph id="ph1">`context`</ph> object parameter is the virtual path for which the configuration section handler computes configuration values.</source>
          <target state="translated"><ph id="ph1">`context`</ph> Parametr obiektu jest ścieżką wirtualną, dla którego program obsługi sekcji konfiguracji oblicza wartości konfiguracji.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Configuration.NameValueSectionHandler.Create(System.Object,System.Object,System.Xml.XmlNode)">
          <source>Normally this parameter is reserved and is <ph id="ph1">`null`</ph>.</source>
          <target state="translated">Zazwyczaj ten parametr jest zarezerwowany i <ph id="ph1">`null`</ph>.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Configuration.NameValueSectionHandler.Create(System.Object,System.Object,System.Xml.XmlNode)">
          <source>The <ph id="ph1">`section`</ph> parameter is the <ph id="ph2">&lt;xref:System.Xml.XmlNode&gt;</ph> object that contains the configuration information to be handled.</source>
          <target state="translated"><ph id="ph1">`section`</ph> Parametr jest <ph id="ph2">&lt;xref:System.Xml.XmlNode&gt;</ph> obiekt, który zawiera informacje o konfiguracji, które mają być obsługiwane.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Configuration.NameValueSectionHandler.Create(System.Object,System.Object,System.Xml.XmlNode)">
          <source>The <ph id="ph1">`section`</ph> parameter provides direct access to the XML contents of the configuration section.</source>
          <target state="translated"><ph id="ph1">`section`</ph> Parametru zapewnia bezpośredni dostęp do zawartości XML sekcji konfiguracji.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="P:System.Configuration.NameValueSectionHandler.KeyAttributeName">
          <source>Gets the XML attribute name to use as the key in a key/value pair.</source>
          <target state="translated">Pobiera nazwa atrybutu XML, który ma być używana jako klucz w parę klucz/wartość.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Configuration.NameValueSectionHandler.KeyAttributeName">
          <source>A <ph id="ph1">&lt;see cref="T:System.String" /&gt;</ph> value containing the name of the key attribute.</source>
          <target state="translated">A <ph id="ph1">&lt;see cref="T:System.String" /&gt;</ph> zawierający nazwę atrybutu klucza.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="P:System.Configuration.NameValueSectionHandler.ValueAttributeName">
          <source>Gets the XML attribute name to use as the value in a key/value pair.</source>
          <target state="translated">Pobiera nazwa atrybutu XML, który ma być używana jako wartość w parę klucz/wartość.</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Configuration.NameValueSectionHandler.ValueAttributeName">
          <source>A <ph id="ph1">&lt;see cref="T:System.String" /&gt;</ph> value containing the name of the value attribute.</source>
          <target state="translated">A <ph id="ph1">&lt;see cref="T:System.String" /&gt;</ph> zawierający nazwę atrybutu value.</target>       </trans-unit>
