      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.Cryptography.CngPropertyOptions">
          <source>Specifies Cryptography Next Generation (CNG) key property options.</source>
          <target state="translated">Określa opcje właściwości klucza kryptografii nowej generacji (CNG).</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Cryptography.CngPropertyOptions">
          <source>These options are used to initialize <ph id="ph1">&lt;xref:System.Security.Cryptography.CngProperty&gt;</ph> objects, which are then used in <ph id="ph2">&lt;xref:System.Security.Cryptography.CngKey.SetProperty%2A&gt;</ph> calls.</source>
          <target state="translated">Te opcje są używane do zainicjowania <ph id="ph1">&lt;xref:System.Security.Cryptography.CngProperty&gt;</ph> obiektów, które są następnie używane w <ph id="ph2">&lt;xref:System.Security.Cryptography.CngKey.SetProperty%2A&gt;</ph> wywołania.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Cryptography.CngPropertyOptions">
          <source>You also use these options when you query for the value of properties with the <ph id="ph1">&lt;xref:System.Security.Cryptography.CngKey.GetProperty%2A&gt;</ph> method.</source>
          <target state="translated">Można również korzystać z tych opcji kwerendy dla wartości właściwości z <ph id="ph1">&lt;xref:System.Security.Cryptography.CngKey.GetProperty%2A&gt;</ph> metody.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Cryptography.CngPropertyOptions">
          <source>Properties must be queried with the <ph id="ph1">&lt;xref:System.Security.Cryptography.CngPropertyOptions&gt;</ph> they are stored with.</source>
          <target state="translated">Właściwości należy zbadać z <ph id="ph1">&lt;xref:System.Security.Cryptography.CngPropertyOptions&gt;</ph> są zapisywane z.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Cryptography.CngPropertyOptions">
          <source>Otherwise, they will not be found.</source>
          <target state="translated">W przeciwnym razie nie znajdują się.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.CngPropertyOptions.CustomProperty">
          <source>The property is not specified by CNG.</source>
          <target state="translated">Nie określono właściwości przez CNG.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.CngPropertyOptions.CustomProperty">
          <source>Use this option to avoid future name conflicts with CNG properties.</source>
          <target state="translated">Użyj tej opcji, aby uniknąć przyszłych nazwa powoduje konflikt z właściwości CNG.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.CngPropertyOptions.None">
          <source>The referenced property has no options.</source>
          <target state="translated">Odwołanie do właściwość nie ma opcji.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.CngPropertyOptions.Persist">
          <source>The property should be persisted.</source>
          <target state="translated">Właściwość powinna być utrwalona.</target>       </trans-unit>
