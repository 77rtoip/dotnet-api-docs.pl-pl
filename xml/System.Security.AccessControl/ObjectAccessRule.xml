        <param name="identity">Tożsamość, do której zostanie zastosowana reguła dostępu.  Musi być obiektem, który może być rzutowany jako <see cref="T:System.Security.Principal.SecurityIdentifier" />.</param>
        <param name="accessMask">Maska dostępu tej reguły. Maska dostępu to 32-bitowa kolekcja anonimowych bitów, której znaczenie jest zdefiniowane przez poszczególne Integratory.</param>
        <param name="isInherited"><see langword="true" />Jeśli ta reguła jest dziedziczona z kontenera nadrzędnego.</param>
        <param name="inheritanceFlags">Określa właściwości dziedziczenia reguły dostępu.</param>
        <param name="propagationFlags">Określa, czy dziedziczone reguły dostępu są automatycznie propagowane. Flagi propagacji są ignorowane, jeśli <paramref name="inheritanceFlags" /> jest ustawiona na <see cref="F:System.Security.AccessControl.InheritanceFlags.None" />.</param>
        <param name="objectType">Typ obiektu, do którego zostanie zastosowana reguła.</param>
        <param name="inheritedObjectType">Typ obiektu podrzędnego, który może dziedziczyć regułę.</param>
        <param name="type">Określa, czy ta reguła zezwala na dostęp.</param>
