      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ComponentModel.Design.DateTimeEditor">
          <source>This date time editor is a <ph id="ph1">&lt;see cref="T:System.Drawing.Design.UITypeEditor" /&gt;</ph> suitable for visually editing <ph id="ph2">&lt;see cref="T:System.DateTime" /&gt;</ph> objects.</source>
          <target state="translated">Jest to edytor czasu data <ph id="ph1">&lt;see cref="T:System.Drawing.Design.UITypeEditor" /&gt;</ph> odpowiednie do edycji wizualnej <ph id="ph2">&lt;see cref="T:System.DateTime" /&gt;</ph> obiektów.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ComponentModel.Design.DateTimeEditor" /&gt;</ph> class.</source>
          <target state="translated">Inicjuje nowe wystąpienie klasy <ph id="ph1">&lt;see cref="T:System.ComponentModel.Design.DateTimeEditor" /&gt;</ph> klasy.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>A type descriptor context that can be used to provide additional context information.</source>
          <target state="translated">Kontekst deskryptora typu, który może służyć do zapewnienia informacji dodatkowy kontekst.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>A service provider object through which editing services may be obtained.</source>
          <target state="translated">Obiekt dostawcy usługi za pomocą których mogą być uzyskane edycji usług.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>An instance of the value being edited.</source>
          <target state="translated">Wystąpienie edytowaną wartością.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>Edits the specified object value using the editor style provided by GetEditorStyle.</source>
          <target state="translated">Umożliwia edycję przy użyciu stylu Edytor podał GetEditorStyle wartość określonego obiektu.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>A service provider is provided so that any required editing services can be obtained.</source>
          <target state="translated">Dostawcy usług są udostępniane, dzięki czemu można uzyskać wszystkie wymagane usługi do edycji.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>The new value of the object.</source>
          <target state="translated">Nowa wartość obiektu.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>If the value of the object hasn't changed, this should return the same object it was passed.</source>
          <target state="translated">Jeśli nie zmieniła się wartość obiektu, powinny zostać zwrócone tego samego obiektu, który został on przekazany.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.GetEditStyle(System.ComponentModel.ITypeDescriptorContext)">
          <source>A type descriptor context that can be used to provide additional context information.</source>
          <target state="translated">Kontekst deskryptora typu, który może służyć do zapewnienia informacji dodatkowy kontekst.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.GetEditStyle(System.ComponentModel.ITypeDescriptorContext)">
          <source>Retrieves the editing style of the <ph id="ph1">&lt;see cref="Overload:System.ComponentModel.Design.DateTimeEditor.EditValue" /&gt;</ph> method.</source>
          <target state="translated">Pobiera styl edycji <ph id="ph1">&lt;see cref="Overload:System.ComponentModel.Design.DateTimeEditor.EditValue" /&gt;</ph> metody.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.GetEditStyle(System.ComponentModel.ITypeDescriptorContext)">
          <source>If the method is not supported, this will return None.</source>
          <target state="translated">Jeśli metoda nie jest obsługiwana, to zwróci None.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.DateTimeEditor.GetEditStyle(System.ComponentModel.ITypeDescriptorContext)">
          <source>An <ph id="ph1">&lt;see langword="enum" /&gt;</ph> value indicating the provided editing style.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="enum" /&gt;</ph> Wskazujące na podany styl edycji.</target>       </trans-unit>
