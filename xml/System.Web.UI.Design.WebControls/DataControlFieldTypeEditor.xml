        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.Design.WebControls.DataControlFieldTypeEditor">
          <source>Provides an editing user interface (UI) for a data control field.</source>
          <target state="translated">Udostępnia interfejs użytkownika (UI) edycji polu formantu danych.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.Design.WebControls.DataControlFieldTypeEditor">
          <source>This editor is used internally by the <ph id="ph1">&lt;xref:System.Web.UI.WebControls.GridView&gt;</ph> and <ph id="ph2">&lt;xref:System.Web.UI.WebControls.DetailsView&gt;</ph> controls.</source>
          <target state="translated">Ten edytor jest używana wewnętrznie przez <ph id="ph1">&lt;xref:System.Web.UI.WebControls.GridView&gt;</ph> i <ph id="ph2">&lt;xref:System.Web.UI.WebControls.DetailsView&gt;</ph> kontrolki.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.DataControlFieldTypeEditor.#ctor">
