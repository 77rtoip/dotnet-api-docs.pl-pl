      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.FormWindowState">
          <source>Specifies how a form window is displayed.</source>
          <target state="translated">Określa sposób wyświetlania okna formularza.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.FormWindowState">
          <source>This enumeration is used by the <ph id="ph1">&lt;xref:System.Windows.Forms.Form&gt;</ph> class.</source>
          <target state="translated">To wyliczenie jest używany przez <ph id="ph1">&lt;xref:System.Windows.Forms.Form&gt;</ph> klasy.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.FormWindowState">
          <source>It represents the different states of the form.</source>
          <target state="translated">Reprezentuje różne stany formularza.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.FormWindowState">
          <source>The default state is <ph id="ph1">`Normal`</ph>.</source>
          <target state="translated">Stan domyślny jest <ph id="ph1">`Normal`</ph>.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.FormWindowState">
          <source>In this example, you change the form's window state to <ph id="ph1">`Maximized`</ph> and display the state information using a label.</source>
          <target state="translated">W tym przykładzie można zmienić stanu okna formularza do <ph id="ph1">`Maximized`</ph> i wyświetlić informacje o stanie za pomocą etykiety.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.FormWindowState">
          <source>This example assumes that you have already created a <ph id="ph1">&lt;xref:System.Windows.Forms.Form&gt;</ph> named <ph id="ph2">`Form1`</ph>.</source>
          <target state="translated">W tym przykładzie przyjęto założenie, że utworzono już <ph id="ph1">&lt;xref:System.Windows.Forms.Form&gt;</ph> o nazwie <ph id="ph2">`Form1`</ph>.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.FormWindowState.Maximized">
          <source>A maximized window.</source>
          <target state="translated">Zmaksymalizowane okno.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.FormWindowState.Minimized">
          <source>A minimized window.</source>
          <target state="translated">Zminimalizowanego okna.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.FormWindowState.Normal">
          <source>A default sized window.</source>
          <target state="translated">Domyślny rozmiar okna.</target>       </trans-unit>
