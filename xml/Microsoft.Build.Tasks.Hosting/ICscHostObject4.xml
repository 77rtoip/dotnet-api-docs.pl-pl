      <MemberSignature Language="F#" Value="abstract member SetPlatformWith32BitPreference : string -&gt; bool" Usage="iCscHostObject4.SetPlatformWith32BitPreference platformWith32BitPreference" />
      <MemberType>Method</MemberType>
